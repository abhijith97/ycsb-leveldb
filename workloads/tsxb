# Yahoo! Cloud System Benchmark
# Workload for measuring leveldb with tsx
#
# load 2M records
# Then we perform 10M random updates, inserts, and reads
# 10% of the operations are writes, 10% inserts, and 80% are reads


recordcount=2000000
operationcount=10000000
workload=com.yahoo.ycsb.workloads.CoreWorkload

readallfields=true

readproportion=0.8
updateproportion=0.1
scanproportion=0
insertproportion=0.1

requestdistribution=zipfian

threadcount=10